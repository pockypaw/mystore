{
    "version": 2,
    "builds": [
      {
        "src": "/backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "/frontend/mystore/dist/**",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/app.js"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/mystore/dist/index.html"
      }
    ]
  }
  